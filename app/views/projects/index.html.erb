<a id="notice"><%= notice %></a>
<div class="jumbotron text-center">
<h1><a class="btn btn-success btn-block" href= "projects/new" role="button">Create New Project</a></h1>
<p>
</p>
</div>

<table>
  <thead>
    <tr>
    
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
  
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
        <td class="btn-group">
        
        
        <button type="button" class="btn btn-light"><%= link_to 'Edit', edit_project_path(project) %></button>
        <button type="button" class="btn btn-light"><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></button>
        </td>
        
      </tr>
    <% end %>

  </tbody>
</table>




